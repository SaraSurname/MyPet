<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PetDayCare"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPet">
        <activity android:name=".InitActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Actualizar"
            android:label="Actualizar"
            android:parentActivityName=".InitActivity"/>
        <activity
            android:name=".Consulta"
            android:label="Consulta"
            android:parentActivityName=".InitActivity"/>
    </application>

</manifest>